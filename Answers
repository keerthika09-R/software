        Thread.sleep(3000);
         By ele=By.xpath("//input[@name='email']");
           helper.javascriptScroll(ele);
           Thread.sleep(6000);
           driver.findElement(By.xpath("//input[@name='email']")).click();
           driver.findElement(By.xpath("//input[@name='email']")).sendKeys("admin@gmail.com");
           Thread.sleep(3000);
           driver.findElement(By.xpath("//button[@id='gumroad-follow-form-embed-button']")).click();
           Thread.sleep(3000);
           Screenshot.getScreenShot(driver, "screenshot");
       

         

    
    }
    @AfterMethod
    public void quitit()
    {
        driver.quit();
    }
}
