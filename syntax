package runner;

import java.io.IOException;
import java.net.MalformedURLException;

import org.openqa.selenium.By;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.Test;

import utils.Base;
import utils.Screenshot;

public class HeatTest extends Base{


    @BeforeMethod
    public void launchBrowser() throws MalformedURLException{
        // launch your browser
       driver= openBrowser();
    }

    @Test
    public void testOne() throws IOException, InterruptedException {
        Screenshot.getScreenShot(driver, "ss1");

        driver.findElement(By.xpath("//input[@class='js-search form-control']")).click();

       Thread.sleep(3000);
   
        driver.findElement(By.xpath("//input[@class='js-search form-control']")).sendKeys("shirt");
        Thread.sleep(3000);
        driver.findElement(By.xpath("(//span[text()='Hot Sauces'])[2]")).click();
                //Thread.sleep(3000);

     


    }

    @AfterMethod
    public void tearDown() {
        // quit the driver here
        driver.quit();
    }
}
