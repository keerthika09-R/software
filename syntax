ALERTS LOGGER
alert:
Alert alert =driver.switchTo().alert();
alert.accept();
driver.switchTo().alert().dismiss();
---------------------------------------
log4j.properties:
log4j.rootLogger=DEBUG,console,fileA,fileB,fileC,fileD

#console appender
log4j.appender.console=org.apache.log4j.consoleAppender
log4j.appender.console.layout=org.apache.log4j.PatternLayout
log4j.appender.console.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss} %p %c -> %n%n


#file appender

log4j.appender.fileA=org.apache.log4j.FileAppender
log4j.appender.fileA.File=logs/first_log_file.log
log4j.appender.fileA.layout=org.apache.log4j.PatternLayout
log4j.appender.fileA.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss} %p %c %C %M -> %n%n

#second log file pattern

log4j.appender.fileB=org.apache.log4j.FileAppender
log4j.appender.fileB.File=logs/second_log_file.log
log4j.appender.fileB.layout=org.apache.log4j.PatternLayout
log4j.appender.fileB.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss} %p %c{3} -> %n%n

#third log file pattern
log4j.appender.fileC=org.apache.log4j.FileAppender
log4j.appender.fileC.File=logs/third_log_file.log
log4j.appender.fileC.layout=org.apache.log4j.PatternLayout
log4j.appender.fileC.layout.ConversionPattern=%d %p %c %C %M -> %n%n

#fourth log file pattern
log4j.appender.fileD=org.apache.log4j.FileAppender
log4j.appender.fileD.File=logs/fourth_log_file.log
log4j.appender.fileD.layout=org.apache.log4j.PatternLayout
